type User {
  id: ID! @unique
  name: String!
  privileges: [Privilege!]!
  actionTypes: [String!]!
  identity: String!,
  auth0id: String!,
  email: String!,
  avatar: String!

}

type Privilege {
  id: ID! @unique
  name: String! 
  users: [User!]!
  actionTypes: [String!]!
  privileges: [Privilege!]!
}
